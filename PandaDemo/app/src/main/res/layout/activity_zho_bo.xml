<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jiyun.qcloud.dashixummoban.ui.home.activity.ZhoBoActivity">
<com.dl7.player.media.IjkPlayerView
    android:id="@+id/zhiboview"
    android:layout_width="match_parent"
    android:layout_height="match_parent" />

    <TextView
        android:id="@+id/titletext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:textColor="@color/baise"
        android:textSize="15sp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y32"
        android:text="" />

    <ImageView
        android:id="@+id/back_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/titletext"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:src="@drawable/fenlei_fanhui" />
</RelativeLayout>
